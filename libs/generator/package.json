{
  "name": "@kubejs/generator",
  "version": "1.1.3",
  "description": "A generator for KubeJS plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/MDReal32/bundled-kubejs.git",
    "directory": "kubejs/generator"
  },
  "type": "module",
  "main": "build/main.js",
  "bin": {
    "kubejs-generator": "bin/generator.js"
  },
  "files": [
    "build",
    "bin",
    "templates"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "publish:me": "yarn npm publish --access public",
    "watch": "vite build --watch"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.8.6",
    "@kubejs/core": "workspace:^",
    "ejs": "3.1.10",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.2",
    "semver": "^7.7.2",
    "simple-git": "^3.28.0",
    "yaml": "^2.8.1",
    "yargs": "18.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/ejs": "^3.1.5",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.7.0",
    "@types/semver": "^7.7.1",
    "@types/yargs": "17.0.33",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "yarn@4.0.2"
}
